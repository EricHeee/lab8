# Dictionary for what each letter would be replaced with
LetterDict = {
    'a':'3', 'b':'!', 'c':'@', 'd':'#', 'e':'1', 'f':'$', 'g':'%', 'h':'9', 'i':'5', 'j':'^',
    'k':'&', 'l':'*', 'm':'+', 'n':'6', 'o':'4', 'p':'?', 'q':'<', 'r':'8', 's':'7', 't':'2', 
    'u':'>', 'v':':', 'w':'M', 'x':'A', 'y':'W', 'z':'E',
    '-':'||', '.': 'a', ',': 'b', '?':'c', '!': 'd'
}
